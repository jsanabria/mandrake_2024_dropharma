  <CustomScript ScriptType="Server" ScriptCodeType="Table" ScriptName="Recordset_Selecting" ScriptL1Key="temp_consignacion"><![CDATA[// Recordset Selecting event
function Recordset_Selecting(&$filter) {
	// Enter your code here

	$sql = "SELECT tipo_documento FROM username_tipo_documento WHERE username = '" . CurrentUserName() . "';";
	$tipo = ExecuteScalar($sql);

	switch($tipo) {
	case "TDCNRP":
		AddFilter($filter, "tipo_documento IN ('TDCPDC', 'TDCFCC')");
		break;
	case "TDCFCC":
		//AddFilter($filter, "tipo_documento IN ('TDCNRP')");
		AddFilter($filter, "(tipo_documento IN ('TDCNRP') OR (tipo_documento IN ('TDCNRP') AND consignacion = 'S' AND consignacion_reportada = 'N'))");
		break;
	default:
		AddFilter($filter, "tipo_documento IN ('N/A')");
	}

	AddFilter($filter, "estatus = 'NUEVO'");
}
]]></CustomScript>
