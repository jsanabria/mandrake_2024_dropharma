		if(!$row = ExecuteRow($sql)) {
			//$this->CancelMessage = "El periodo contable no existe; verifique.";
			return FALSE;
		}
		else { 
			if($row["cerrado"] == "S") {
				//$this->CancelMessage = "El periodo contable est&aacute; cerrado; verifique.";
				return FALSE;
			}
