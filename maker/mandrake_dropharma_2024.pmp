							$cod_articulo = $datos[$columna];
							break;
						case 1:	
							$nombre = $datos[$columna];
							break;
						case 2:	
							$cantidad = intval($datos[$columna]);
							break;
						}
					}
					$insart->insertar_articulo("TDCPDC", $rsnew["id"], $rsnew["proveedor"], $cod_articulo, $cantidad);
				}
				$codigo = "";
				$nombre = "";
