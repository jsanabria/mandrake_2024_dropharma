  <CustomScript ScriptType="Template" ScriptCodeType="Table" ScriptName="Content" ScriptCtrlID="custom" ScriptL1Key="codigo_buscar.php"><![CDATA[<?php
$codigo = $_GET["codigo"];
$accion = $_GET["accion"];

$sql = "SELECT COUNT(codigo) AS cantidad FROM articulo WHERE codigo = '$codigo';";
$cantidad = ExecuteScalar($sql);

if($cantidad > ($accion == "I" ? 0 : 1)) $cantidad = 1;

$out = '<div id="outtext">' . $cantidad . '</div>';
echo $out;
?>
]]></CustomScript>
