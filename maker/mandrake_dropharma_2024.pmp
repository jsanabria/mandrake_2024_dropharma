    <Table TblName="view_existencia_almacen" TblType="VIEW" TblGen="1" TblGenList="controller=0,info=0,entity=0,list=0,list|class=0" TblList="1" TblCaption="Existencia por Almacen" TblUpdatable="1" TblSQL="select `a`.`id` AS `id_compra`,`a`.`almacen` AS `codalm`,(select `almacen`.`descripcion` from `almacen` where (`almacen`.`codigo` = `a`.`almacen`)) AS `nom_almacen`,`a`.`fabricante` AS `codfab`,`a`.`articulo` AS `codart`,`fb`.`nombre` AS `fabricante`,`ar`.`codigo` AS `codigo`,`ar`.`codigo_de_barra` AS `codigo_de_barra`,`ar`.`nombre_comercial` AS `nombre_comercial`,`ar`.`principio_activo` AS `principio_activo`,`ar`.`presentacion` AS `presentacion`,`ar`.`principio_activo` AS `articulo`,`a`.`lote` AS `lote`,date_format(`a`.`fecha_vencimiento`,'%d/%m/%Y') AS `fecha_vencimiento`,(ifnull(`a`.`cantidad_movimiento`,0) + ifnull(`d`.`cantidad_movimiento`,0)) AS `cantidad` from (((((`entradas_salidas` `a` join `entradas` `b` on(((`b`.`tipo_documento` = `a`.`tipo_documento`) and (`b`.`id` = `a`.`id_documento`)))) join `almacen` `c` on(((`c`.`codigo` = `a`.`almacen`) and (`c`.`movimiento` = 'S')))) left join `articulo` `ar` on((`ar`.`id` = `a`.`articulo`))) left join `fabricante` `fb` on((`fb`.`Id` = `a`.`fabricante`))) left join (select `a`.`id_compra` AS `id`,sum(ifnull(`a`.`cantidad_movimiento`,0)) AS `cantidad_movimiento` from ((`entradas_salidas` `a` join `salidas` `b` on(((`b`.`tipo_documento` = `a`.`tipo_documento`) and (`b`.`id` = `a`.`id_documento`)))) left join `almacen` `c` on(((`c`.`codigo` = `a`.`almacen`) and (`c`.`movimiento` = 'S')))) where ((`a`.`tipo_documento` in ('TDCNET','TDCASA')) and (`b`.`estatus` in ('NUEVO','PROCESADO'))) group by `a`.`id_compra`) `d` on((`d`.`id` = `a`.`id`))) where ((((`a`.`tipo_documento` in ('TDCNRP','TDCAEN')) and (`b`.`estatus` = 'PROCESADO')) or ((`a`.`tipo_documento` in ('TDCNRP','TDCAEN')) and (`b`.`estatus` &lt;&gt; 'ANULADO') and (`b`.`consignacion` = 'S'))) and ((ifnull(`a`.`cantidad_movimiento`,0) + ifnull(`d`.`cantidad_movimiento`,0)) &gt; 0)) order by `a`.`fecha_vencimiento`" TblSrchType="BASIC" TblBasicSearch="1" TblExtendedBasicSearch="1" TblSecurity="0,&quot;Default&quot;,0" TblLoaded="1" TblShowEmptyRows="0" TblUseGlobal="1" TblRecPerPage="20" TblRecPerPageList="10,20,50,ALL" TblGrpPerPage="3" TblGrpPerPageList="1,2,3,5,ALL" TblPagerStyle="2" TblUseButtonsForLinks="1" TblUseDropDownForAction="1" TblUseDropDownForListOptions="1" TblUseDropDownForExport="1">
