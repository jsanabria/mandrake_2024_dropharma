	}

	$fecha_inicio = new \DateTime($rsnew["fecha_inicio"]);
	$fecha_fin = new \DateTime($rsnew["fecha_fin"]);	

	if($fecha_inicio > $fecha_fin) {
		$this->CancelMessage = "La fecha desde debe ser menor a la fecha hasta; verifique.";
		return FALSE;
	}

	$sql = "SELECT MAX(fecha_fin) AS fecha FROM cont_periodo_contable WHERE cerrado = 'S';";
	$fecha = new \DateTime(ExecuteScalar($sql));
	if($fecha_inicio <= $fecha) {
		$this->CancelMessage = "La fecha inicio del periodo contable debe ser mayor al cierre del periodo actual; verifique.";
		return FALSE;
	}

	$diff = $fecha_inicio->diff($fecha_fin);
	$anhos = $diff->y;
	$meses = $diff->m;
	$dias = $diff->d;

	if($meses < 11) {
		$this->CancelMessage = "El periodo contable debe ser de 12 meses; verifique.";
		return FALSE;
	}
	
	$rsnew["cerrado"] = "N";
	
	return TRUE;
}
]]></CustomScript>
