function Row_Inserting($rsold, &$rsnew) {
	// Enter your code here
	// To cancel, set return value to FALSE

	$clase = trim($rsnew["clase"]);
	$grupo = trim($rsnew["grupo"]);
	$cuenta = trim($rsnew["cuenta"]);
	$subcuenta = trim($rsnew["subcuenta"]);

	$where = "";
	if($grupo != "") $where .= "AND LTRIM(RTRIM(grupo)) = '$grupo' ";
	if($cuenta != "") $where .= "AND LTRIM(RTRIM(cuenta)) = '$cuenta' ";
	if($subcuenta != "") $where .= "AND LTRIM(RTRIM(subcuenta)) = '$subcuenta' ";
	$sql = "SELECT 
				COUNT(descripcion) AS cantidad 
			FROM 
				cont_plancta 
			WHERE 
				LTRIM(RTRIM(clase)) = '$clase' 
				$where;";
	$cantidad = intval(ExecuteScalar($sql));

	if($cantidad >= 1) {
		$this->CancelMessage = "La cuenta ya existe; !Verifique!";
		return FALSE;
	}

	switch($clase) {
	case '1':
		$rsnew["clasificacion"] = "ACTIVO";
		$rsnew["naturaleza"] = "DEBE";
		$rsnew["tipo"] = "BALANCE";
		break;
	case '2':
		$rsnew["clasificacion"] = "PASIVO";
		$rsnew["naturaleza"] = "HABER";
		$rsnew["tipo"] = "BALANCE";
		break;
	case '3':
		$rsnew["clasificacion"] = "CAPITAL";
		$rsnew["naturaleza"] = "HABER";
		$rsnew["tipo"] = "BALANCE";
		break;
	case '4':
		$rsnew["clasificacion"] = "VENTAS";
		$rsnew["naturaleza"] = "DEBE";
		$rsnew["tipo"] = "RESULTADO";
		break;
	case '5':
		$rsnew["clasificacion"] = "COSTOS";
		$rsnew["naturaleza"] = "HABER";
		$rsnew["tipo"] = "RESULTADO";
		break;
	case '6':
		$rsnew["clasificacion"] = "GASTOS";
		$rsnew["naturaleza"] = "HABER";
		$rsnew["tipo"] = "RESULTADO";
		break;
	}

	return TRUE;
}
