					}
					break;
				case "200": // IVA crédito y débito
					if($montos["tipo_trasn"] == "NC") {
						if($cargo == "DEBE") $cargo = "HABER";
						else $cargo = "DEBE";
					}
					
					if($cargo == "DEBE") $debe = $montos["monto_iva"];
