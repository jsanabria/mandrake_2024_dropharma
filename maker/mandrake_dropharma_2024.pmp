  <CustomScript ScriptType="Server" ScriptCodeType="Table" ScriptName="Page_Load" ScriptCtrlID="list" ScriptL1Key="view_out_tdcfcv"><![CDATA[// Page Load event
function Page_Load()
{

    // Verificar si la acción es "exportar"
    if (Get("action") == "export" && Get("tipo_documento") == "TDCFCV") {
        
        // Obtener las fechas del formulario usando JQuery,
        // que PHPMaker típicamente maneja a través de las variables de POST/GET
        // Sin embargo, para obtener los valores del filtro actual en la página de lista
        // necesitamos usar los IDs de los campos de filtro que PHPMaker usa:
        
        // Suponiendo que los filtros son los campos "fecha" y tienen los IDs x_fecha y y_fecha
        $fecha_desde = Post("x_fecha") ?? Get("x_fecha") ?? ""; // Asume que se usa el filtro de búsqueda/lista
        $fecha_hasta = Post("y_fecha") ?? Get("y_fecha") ?? "";
        
        // --- Validación de Fechas ---
        if (empty($fecha_desde) || empty($fecha_hasta)) {
            // Debes mostrar un mensaje de error dentro del contexto de PHPMaker.
            // Usamos la función de error de PHPMaker (SetFailureMessage) y redirigimos.
            // SetFailureMessage("¡Atención! Debe seleccionar un rango de fechas ('Desde' y 'Hasta') en el filtro para poder descargar el reporte.");
            $_SESSION[Config("SESSION_FAILURE_MESSAGE")] = "¡Atención! Debe seleccionar un rango de fechas ('Desde' y 'Hasta') en el filtro para poder descargar el reporte.";
            
            // Redirige para mostrar el mensaje de error en la página actual
            Redirect(CurrentPageUrl()); 
            return; // Detiene la ejecución para que la redirección surta efecto
        }

        // --- Preparación de la Consulta ---
        
        // PHPMaker necesita la fecha en formato YYYY-MM-DD para la base de datos
        // Aquí asumimos que $fecha_desde y $fecha_hasta ya están en formato compatible o los convertimos si es necesario.
        // Si la entrada es DD/MM/YYYY, usa DateFormat("Y-m-d", $fecha_desde) (función nativa de PHPMaker)
        
        $fecha_desde_db = DateFormat("Y-m-d", $fecha_desde);
        $fecha_hasta_db = DateFormat("Y-m-d", $fecha_hasta);
        
        $sql = "SELECT 
            a.cliente AS CODIGO_CLIENTE, 
            DATE_FORMAT(a.fecha, '%d/%m/%Y') AS FECHAEMISION, 
            DATE_ADD(a.fecha, INTERVAL a.dias_credito DAY) AS F_VENCIM, 
            a.nro_documento AS NoFact, 
            a.nro_control AS NoControl, 
            a.total AS MONTODOCUMENTO_BS, 
            a.monto_total AS BIMP, 
            a.iva AS IVA, 
            a.monto_usd AS MONTO_EN_$, 
            a.tasa_dia AS FACTOR_DE_CAMBIO 
        FROM 
            salidas AS a 
        WHERE 
            a.tipo_documento = 'TDCFCV' AND a.estatus = 'PROCESADO' AND 
            a.fecha BETWEEN '" . AdjustSql($fecha_desde_db) . "' AND '" . AdjustSql($fecha_hasta_db) . "' 
        ORDER BY a.nro_control";
        
        // --- Ejecución y Exportación ---
        
        // Usar la función de base de datos de PHPMaker para obtener los registros
        $results = ExecuteRows($sql); 
        
        // PHPMaker usa GetDb() para obtener la conexión
        $db = GetDb();
        
        // 1. Cabeceras HTTP para la descarga del archivo CSV (compatible con Excel)
        header('Content-Type: text/csv; charset=utf-8');
        header('Content-Disposition: attachment; filename="reporte_ventas_TDCFCV.csv"');

        // 2. Abrir el "output"
        $output = fopen('php://output', 'w');

        // 3. Agregar el BOM de UTF-8 para Excel (Crucial para acentos)
        fputs($output, $bom = (chr(0xEF) . chr(0xBB) . chr(0xBF)));

        // 4. Escribir las Cabeceras (deben coincidir con el SELECT)
        $headers = [
            'CODIGO_CLIENTE', 'FECHAEMISION', 'F_VENCIM', 'NoFact', 'NoControl', 
            'MONTODOCUMENTO_BS', 'BIMP', 'IVA', 'MONTO_EN_$', 'FACTOR_DE_CAMBIO'
        ];
        // Usamos ';' como delimitador para compatibilidad con Excel en LATAM
        fputcsv($output, $headers, ';'); 

        // 5. Escribir los datos
        if ($results) {
            foreach ($results as $row) {
                // fputcsv espera un array indexado numéricamente o asociativamente
                fputcsv($output, $row, ';'); 
            }
        }

        // 6. Cerrar y salir
        fclose($output);
        exit(); 
    }

}]]></CustomScript>
